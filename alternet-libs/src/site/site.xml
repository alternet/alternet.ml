<?xml version="1.0" encoding="UTF-8"?>
<project name="Alternet Libs" xmlns="http://maven.apache.org/DECORATION/1.3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

    <bannerLeft>
        <name><![CDATA[
      <span class="color-highlight">Alternet</span> Libs
      ]]>
        </name>
        <href>http://alternet.ml/alternet-libs</href>
    </bannerLeft>

    <publishDate position="bottom" format="yyyy-MM-dd" />
    <version position="bottom" />
    <!-- googleAnalyticsAccountId></googleAnalyticsAccountId -->

    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <!-- Make this to 'false' for local development, i.e. file:// URLs -->
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>false</smoothScroll>
            <theme>bootswatch-cerulean</theme>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>solarized_dark</highlightJsTheme><!--github -->
            <!-- absoluteResourceURL>http://alternet.ml/alternet/</absoluteResourceURL -->
            <brand>
                <!-- Brand text in top-left part of the site -->
                <name>
          <![CDATA[
          <span class="color-highlight">Alternet</span> Libs
          ]]>
                </name>
                <href>http://alternet.ml/alternet-libs</href>
            </brand>
            <slogan>Common Alternet Libraries</slogan>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <!-- Use Table of Contents at the top of the page (max 6 elements) -->
            <toc>top</toc>
            <tocTopMax>5</tocTopMax>
            <!-- Include the documentation and tools in the top navigation (in addition to links) -->
            <topNav>Documentation|Tools|Contribute</topNav>
            <!-- Split menus in the bottom navigation -->
            <bottomNav maxSpan="9">
                <column>Main|Download</column>
                <column>Documentation</column>
                <column>reports</column>
                <column>Projects|Misc</column>
            </bottomNav>
            <skinAttribution>false</skinAttribution>
            <bottomDescription>
        <![CDATA[
          <span class="color-highlight">Alternet Libs</span> is a bunch of common Java libraries.
        ]]>
            </bottomDescription>
            <pages>
                <index>
                    <sections>
                        <body />
                        <columns>3</columns>
                        <columns>3</columns>
                    </sections>
                    <titleTemplate>Alternet Libs</titleTemplate>
                    <breadcrumbs>false</breadcrumbs>
                    <markPageHeader>false</markPageHeader>
                    <smoothScroll>false</smoothScroll>
                    <toc>false</toc>
                </index>
                <contribute>
                    <sections>
                        <body />
                        <columns>3</columns>
                        <columns>3</columns>
                    </sections>
                    <!-- Override the title -->
                    <titleTemplate>Alternet Libs</titleTemplate>
                    <breadcrumbs>true</breadcrumbs>
                    <markPageHeader>false</markPageHeader>
                    <smoothScroll>false</smoothScroll>
                    <toc>false</toc>
                </contribute>
                <demo>
                    <titleTemplate>Alternet Demos</titleTemplate>
                    <breadcrumbs>true</breadcrumbs>
                    <markPageHeader>false</markPageHeader>
                    <smoothScroll>false</smoothScroll>
                    <toc>true</toc>
                </demo>
                <download>
                    <titleTemplate>Alternet Download</titleTemplate>
                    <breadcrumbs>true</breadcrumbs>
                    <markPageHeader>false</markPageHeader>
                    <smoothScroll>false</smoothScroll>
                    <toc>true</toc>
                </download>
                <related>
                    <titleTemplate>Related Projects</titleTemplate>
                    <breadcrumbs>true</breadcrumbs>
                    <markPageHeader>true</markPageHeader>
                    <smoothScroll>true</smoothScroll>
                    <toc>true</toc>
                </related>
                <!-- Disable source highlighting for Maven reports -->
                <source-repository>
                    <highlightJs>false</highlightJs>
                </source-repository>
                <issue-tracking>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </issue-tracking>
                <license>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </license>
                <!-- Disable ToC for some Maven reports -->
                <project-info>
                    <toc>false</toc>
                </project-info>
                <github-report>
                    <toc>false</toc>
                </github-report>
                <dependencies>
                    <tocTopMax>4</tocTopMax>
                </dependencies>
            </pages>
        </reflowSkin>
    </custom>

    <body>
        <!-- Add a rel to Google profile for all pages -->
        <head>
            <link rel="author" href="https://plus.google.com/108799833576409207321" />
        </head>

        <links>
            <item name="Download" href="download.html" />
            <item name="GitHub project" href="https://github.com/alternet/alternet-ml" />
        </links>

        <breadcrumbs>
            <item name="Alternet" href=".." />
            <item name="Alternet Libs" href="index.html" />
        </breadcrumbs>

        <menu name="Main" inherit="top">
            <item name="Home" href="./" />
            <item name="GitHub project" href="https://github.com/alternet/alternet-libs" />
            <item name="Blog" href="http://semantic-mismatch.blogspot.fr/" />
            <item name="Release notes" href="release-notes.html" />
        </menu>
        <menu name="Download" inherit="top">
            <item name="Download" href="download.html" />
            <item name="License" href="license.html" />
        </menu>
        <menu name="Documentation" inherit="bottom">
<!--             <item name="Alternet Crypto" href="crypto/crypto.html" /> -->
            <item name="Alternet Scanner" href="scanner/index.html" />
            <item name="Alternet Security" href="security/index.html" />
            <item name="Alternet Tools" href="tools/index.html" />
        </menu>
        <menu name="Maven reports" ref="reports" inherit="bottom" />
        <menu name="Projects" inherit="bottom">
            <item name="RefleX" href="http://reflex-x.ml/" />
<!--             <item name="UFoS" href="http://ufos.ml/" /> -->
            <item name="Alternet Libs" href="http://alternet.ml/alternet-libs/"/>
        </menu>
        <menu name="Misc" inherit="bottom">
            <item name="Demos" href="demo.html" />
        </menu>
    </body>
</project>
